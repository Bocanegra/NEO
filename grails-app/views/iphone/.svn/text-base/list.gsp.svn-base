<ul id="smes" title="SME list">
  <g:each in="${data}" status="i" var="smeInstance">
    <li>
      <g:link action="renderContent.iphone" params="[sme:smeInstance.cif, employee:employee.nif, smeToRender:smeInstance.cif]">
        ${smeInstance.name}
      </g:link>
    </li>
  </g:each>
</ul>