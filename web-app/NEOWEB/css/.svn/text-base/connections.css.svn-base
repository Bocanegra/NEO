}

.tituloComponenteN{	
	fontFamily: TrebuchetMS;
	fontSize: 25;
   	fontWeight: normal;
   	fontStyle: normal;
   	color: #78A2E7;
   	height: 30;	
}

.contenedorComponenteN{
	horizontalScrollPolicy: off;
	verticalScrollPolicy: off;	
	paddingBottom: 3;
	paddingLeft: 3;
	paddingRight: 3;
	paddingTop: 3;	
}

.fondoComponenteN{
	borderColor: #2256AC;
	borderStyle: solid;
	borderThickness: 2;
	cornerRadius: 20;
	backgroundColor: #000064;
	horizontalScrollPolicy: off;
	verticalScrollPolicy: off;
	backgroundAlpha: 0.7;
	dropShadowEnabled: true;
}
.fondoMenuN{
	borderColor: #000064;
	borderStyle: solid;
	borderThickness: 3;
	cornerRadius: 10;
	backgroundColor: #D9D9D9;
	backgroundAlpha: 0.95;
	dropShadowEnabled: true;
}
.menuVerticalN{
	borderColor: #f2f2f2;
	borderStyle: solid;
	borderThickness: 1.5;
	cornerRadius: 10;
	backgroundColor: #D9D9D9;
	backgroundAlpha: 0.95;
	horizontalAlign: left;
	verticalAlign: bottom;
	verticalGap: -6;
	paddingLeft: 6;
	paddingTop: 6;
	paddingBottom: 6;
}

.menuHorizontalN{
	borderColor: #f2f2f2;
	borderStyle: solid;
	borderThickness: 1.5;
	cornerRadius: 10;
	backgroundColor: #D9D9D9;
	backgroundAlpha: 0.95;
	horizontalAlign: left;
	verticalAlign: middle;
	verticalGap: -6;
	paddingLeft: 6;
	paddingTop: 6;
	paddingBottom: 6;
}

