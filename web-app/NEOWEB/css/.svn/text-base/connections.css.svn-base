.formItemN {
	fontFamily: TrebuchetMS;
	fontSize: 13;
   	fontWeight: normal;
   	paddingTop: 2;
   	paddingRight: 2;
   	paddingBottom: 2;
   	paddingLeft: 2;
   	color: #FFFFFF;
}

.formHeadingN{
	fontFamily: TrebuchetMS;
	fontSize: 18;
   	fontWeight: bold;
   	fontStyle: italic;
   	color: #FFFFFF;
}

.comboBoxN{
	fontFamily: TrebuchetMS;
	fontSize: 13;
   	fontWeight: normal;
   	editable: false;
   	borderThickness: 1.5;
   	color: #FFFFFF;
	themeColor: #78A2E7;
	borderColor: #78A2E7;
	fillColors: #000080, #78A2E7;
	fillAlphas: 1.0, 1.0;
    selectionColor: #000080;
    textSelectedColor: #aad7e5;
    rollOverColor: #27279a;
    textRollOverColor: #aad7e5;
    alternatingItemColors: #5e8dd0, #c1bcbc;
}

.checkBoxN{
	iconColor: #FFFFFF;
   	themeColor: #DCDCDC;
	fontFamily: TrebuchetMS;
	fontSize: 14;
   	fontWeight: normal;
   	fontStyle: normal;
   	color: #FFFFFF;
   	textRollOverColor: #000000;   	
}

.listN{
	borderStyle: none;
	cornerRadius: 20;
	backgroundColor: #F2F2F2;
	backgroundAlpha: 0.2;
}

.accordionN{
   	cornerRadius: 10;
	paddingBottom: 2;
	paddingLeft: 2;
	paddingRight: 2;
	paddingTop: 2;
   	headerHeight: 25;
   	backgroundColor: #000080;
   	borderColor: #000064;
   	headerStyleName: accordionHeaderN;
	dropShadowEnabled: true;
	kerning: false;
	textRollOverColor: #000000;
}

.accordionHeaderN{
	fontFamily: TrebuchetMS;
	fontSize: 14;
   	fontWeight: normal;
   	textAlign: center;
   	color: #FFFFFF;
	fillColors: #000080, #78A2E7;
	fillAlphas: 1.0, 1.0;
	cornerRadius: 20;
	textSelectedColor: #000080;
	themeColor: #78A2E7
}

.dataGridN{
	Alpha: 0.75;
	borderStyle: solid;
	borderThickness: 3; 
	alternatingItemColors: #F2F2F2, #D0DFF7;
	textSelectedColor: #000080;
	borderColor: #D0DFF7;
	color: #000000;
	themeColor: #78A2E7;
	backgroundAlpha: 0.4;
	fontFamily: TrebuchetMS;
	fontSize: 12;
	fotntWeight: normal;
	backgroundColor: #2256AC;
	headerStyleName: DataGridHeaderN;
}

.DataGridHeaderN{
	fontFamily: TrebuchetMS;
	fontSize: 13;
	fotntWeight: bold;
	textAlign: center;
}

