.contenedorTituloC{
	verticalAlign: middle;
	paddingRight: 10;	
	paddingLeft: 10;	
}

.tituloComponenteC{	
	fontFamily: TrebuchetMS;
	fontSize: 25;
   	fontWeight: normal;
   	fontStyle: normal;
   	color: #00C800;
}

.contenedorComponenteC{
	horizontalScrollPolicy: off;
	verticalScrollPolicy: off;	
	paddingBottom: 3;
	paddingLeft: 3;
	paddingRight: 3;
	paddingTop: 3;	
}

.fondoComponenteC{
	borderColor: #008B00;
	borderStyle: solid;
	borderThickness: 2;
	cornerRadius: 20;
	backgroundColor: #F2F2F2;
	horizontalScrollPolicy: off;
	verticalScrollPolicy: off;
	backgroundAlpha: 0.1;
	dropShadowEnabled: true;
}
.fondoMenuC{
	borderColor: #008B00;
	borderStyle: solid;
	borderThickness: 3;
	cornerRadius: 10;
	backgroundColor: #D9D9D9;
	backgroundAlpha: 0.95;
	dropShadowEnabled: true;
}
.menuVerticalC{
	borderColor: #f2f2f2;
	borderStyle: solid;
	borderThickness: 1.5;
	cornerRadius: 10;
	backgroundColor: #D9D9D9;
	backgroundAlpha: 0.95;
	horizontalAlign: left;
	verticalAlign: bottom;
	verticalGap: -6;
	paddingLeft: 6;
	paddingTop: 6;
	paddingBottom: 6;
}

.menuHorizontalC{
	borderColor: #f2f2f2;
	borderStyle: solid;
	borderThickness: 1.5;
	cornerRadius: 10;
	backgroundColor: #D9D9D9;
	backgroundAlpha: 0.95;
	horizontalAlign: left;
	verticalAlign: middle;
	verticalGap: -6;
	paddingLeft: 6;
	paddingTop: 6;
	paddingBottom: 6;
}

