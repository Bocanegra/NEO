.formItemC {
	fontFamily: TrebuchetMS;
	fontSize: 13;
   	fontWeight: normal;
   	paddingTop: 2;
   	paddingRight: 2;
   	paddingBottom: 2;
   	paddingLeft: 2;
   	color: #FFFFFF;
}

.formHeadingC{
	fontFamily: TrebuchetMS;
	fontSize: 18;
   	fontWeight: bold;
   	fontStyle: italic;
   	color: #FFFFFF;
}


.comboBoxC{
	fontFamily: TrebuchetMS;
	fontSize: 13;
   	fontWeight: normal;
   	editable: false;
   	borderThickness: 1.5;
   	color: #FFFFFF;
   	themeColor: #DCDCDC;
  	borderColor: #040404;
	fillColors: #00C800, #008B00;   	
	fillAlphas: 1.0, 1.0;
    selectionColor: #009900;
    textSelectedColor: #000000;
    rollOverColor: #009900;
    textRollOverColor: #000000;
    alternatingItemColors: #65c12f, #c1bcbc;
}

.checkBoxC{
	iconColor: #000000;
   	themeColor: #DCDCDC;
	fontFamily: TrebuchetMS;
	fontSize: 14;
   	fontWeight: normal;
   	fontStyle: normal;
   	color: #FFFFFF;
	textRollOverColor: #008C00;   	
}

.listC{
	borderStyle: none;
	cornerRadius: 20;
	backgroundColor: #F2F2F2;
	backgroundAlpha: 0.2;
}

.accordionC{
   	cornerRadius: 10;
	paddingBottom: 2;
	paddingLeft: 2;
	paddingRight: 2;
	paddingTop: 2;
   	headerHeight: 25;   	
   	backgroundColor: #008B00;
   	borderColor: #FAFAFA;   	
   	headerStyleName: accordionHeaderC;
	dropShadowEnabled: true;
	kerning: false;
	textRollOverColor: #020202;
}

.accordionHeaderC{
	fontFamily: TrebuchetMS;
	fontSize: 14;
   	fontWeight: normal;
   	textAlign: center;
   	color: #FFFFFF;
   	fillColors: #008B00, #008B00;
	fillAlphas: 1.0, 1.0;
	cornerRadius: 20;
	textSelectedColor: #008B00;
	themeColor: #FAFAFA
}

 .dataGridC{
	Alpha: 0.75;
	backgroundAlpha: 0.4;
	borderStyle: solid;
	borderThickness: 3; 
	fontFamily: TrebuchetMS;
	fontSize: 12;
	fotntWeight: normal;
	borderColor: #000000;
	color: #000000;
    selectionColor: #008B00;
    textSelectedColor: #000000;
    rollOverColor: #009900;
    textRollOverColor: #000000;
	alternatingItemColors: #65c12f, #c1bcbc;    
	headerStyleName: DataGridHeaderC;
}


.DataGridHeaderC{
	fontFamily: TrebuchetMS;
	fontSize: 13;
	fotntWeight: bold;
	textAlign: center;
}

