Application {   /* backgroundImage: Embed(source="assets/backgrounds.swf#bluestripe"); */   fontFamily: TrebuchetMS;   fontSize: 15;}

Panel {   borderColor: #D99133;   borderAlpha: 1.0;   borderThicknessLeft: 2;   borderThicknessTop: 2;   borderThicknessBottom: 2;   borderThicknessRight: 2;   roundedBottomCorners: false;   cornerRadius: 10;   headerHeight: 25;   backgroundAlpha: 1.0;   backgroundColor: #ffffff;   dropShadowEnabled: true;   shadowDirection: right;   titleStyleName: "mypanelTitle";}Alert {	fontFamily: TrebuchetMS;	fontSize: 15;	textAlign: center;	buttonStyleName: "myPanelTitle";	titleStyleName: "mypanelTitle";	dropShadowEnabled: true;	shadowDistance: 5;	shadowdirection: right;}ComboBox {	fontFamily: TrebuchetMS;	fontSize: 15;	textInputStyleName: "blackLabel"}	Label {	fontFamily: TrebuchetMS;	fontSize: 15;}Button {	fontFamily: TrebuchetMSbold;	fontSize: 15;}FormItem {	labelStyleName: "orangeLabel";}.mypanelTitle {   fontFamily: TrebuchetMSbold;   fontSize: 15;   fontWeight: bold;}.orangeButton {	fillAlphas: 0.65, 1.0, 0.65, 0.65;	fillColors: #F5A235, #BA7823;}.orangeLabel {	fontFamily: TrebuchetMSbold;	fontSize: 15;	color: #D99133;	fontWeight: bold;}	.blackLabel {	fontFamily: TrebuchetMSbold;	fontSize: 15;	color: #000000;	fontWeight: bold;}	.orangeTitle {	fontFamily: TrebuchetMSbold;	color: #D99133;	fontWeight: bold;	}	@font-face {   src: local("Trebuchet MS");   fontFamily: "TrebuchetMSbold";   fontWeight: bold;}@font-face {   src: local("Trebuchet MS");   fontFamily: "TrebuchetMS";}ToggleButtonBar {   fontFamily: TrebuchetMS;   fontSize: 15;   color: #555555;   buttonHeight: 29;   buttonWidth: 137;   horizontalGap: 20;   selectedButtonTextStyleName: "mySelectedButtonTextStyle";}.mySelectedButtonTextStyle {	fontFamily: TrebuchetMSbold;	fontSize: 17;	color: #000000;	fontWeight: bold;	}.mytoggleButtonBarButtonStyle {   fillAlphas: 0.61, 0.64, 0.75, 0.65;   fillColors: #ffffff, #d99133, #ffffff, #ffffff;   color: #666666;   textRollOverColor: #ff6600;   textSelectedColor: #ffffff;   themeColor: #ff6600;   fontFamily: TrebuchetMS;   fontSize: 15;}.mytoggleButtonBarFirstButtonStyle {}.mytoggleButtonBarLastButtonStyle {}.mytoggleButtonBarSelectedButtonStyle {   fontFamily: TrebuchetMSBold;   fontSize: 15;   fontWeight: bold;   color: #D99133;}